[<%= @name %>]
        <% if @disabled %>DISABLED<% else %>ENABLED<% end %>
        CMD $XYMONCLIENTHOME/ext/<%= @name %>
        <% unless @onhost.nil? %>ONHOST <%= @onhost %><% end %>
        <% unless @maxtime.nil? %>MAXTIME <%= @maxtime %><% end %>
        <% unless @needs.nil? %>NEEDS <%= @needs %><% end %>
        <% unless @group.nil? %>GROUP <%= @group %><% end %>
        <% unless @interval.nil? %>INTERVAL <%= @interval %><% end %> 
        <% unless @crondate.nil? %>CRONDATE <%= @crondate %><% end %> 
        ENVFILE $XYMONCLIENTHOME/etc/xymonclient.cfg
        <% unless @envarea.nil? %>ENVAREA <%= @envarea %><% end %>
        LOGFILE $XYMONCLIENTLOGS/<%= @name %>.log
